/**index.wxss**/

page {
  height: 100%;
  width: 100%;
}
.headImg {
  width: 100%;
  height: fit-content;
  display: flex;
  margin-bottom: 0rem;
}
.top {
  width: 100%;
  height: 240px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: white;
  margin-bottom: 0rpx;
}
.topimage {
  width: 100%;
  margin-left: 0rpx;
  margin-right: 0rpx;
}
.page {
  padding: 20rpx;
  background: #eee;
  position: relative;
}
.page .iconS {
  position: absolute;
  left: 50rpx;
}
.page .inp {
  width: 90%;
  margin: 0 auto;
  border: 1px solid #eee;
  border-radius: 50rpx;
  background: #fff;
  padding: 5rpx 0rpx 5rpx 60rpx;
  font-size: 16px;
}
.indexBlock image {
  width: 100%;
  display: flex;
  position: relative;
  bottom: 0;
}
.indexblock {
  width: 100%;
  position: absolute;
  bottom: 40px;
}
.block {
  float: left;
  width: 25%;
  height:74px;
  color: transparent;
  /* background: #eee; */
}
.conImg{
  font-size: 0;
}
.conImg image ,.bottom2{
  width: 100%;
}
.bottom2 i-icon .i-icon{
  line-height:24px; 
}
